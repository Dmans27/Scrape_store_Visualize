import sqlite3
import pandas as pd



file_path = '/Users/danielmansfield/NVDA__history.csv'  # Add `sheet_name="Sheet1"` if needed

df = pd.read_csv(file_path)


# Connect to (or create) a database
conn = sqlite3.connect("NVDAHISTORY2025.db")

# Save the DataFrame to SQL
df.to_sql("History", conn, if_exists="replace", index=False)

conn.close()
